{
  "name": "php-docblocker2",
  "displayName": "PHP DocBlocker2",
  "description": "A simple, dependency free PHP specific DocBlocking package",
  "version": "2.1.10",
  "license": "MIT",
  "publisher": "tianyiw",
  "author": "tianyiw <tianyiw@vip.qq.com> & Neil Brayfield <dev@brayfield.uk>",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "php",
    "autocomplete",
    "docblock"
  ],
  "activationEvents": [
    "onLanguage:php",
    "onLanguage:hack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tianyiw2013/vscode-php-docblocker"
  },
  "bugs": {
    "url": "https://github.com/tianyiw2013/vscode-php-docblocker/issues"
  },
  "icon": "images/logo.png",
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "php-docblocker2.trigger",
        "title": "%Insert PHP Docblock%"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "PHP DocBlocker2",
      "properties": {
        "php-docblocker2.autoIgnorePropertyType": {
          "type": "boolean",
          "default": true,
          "description": "%option.autoIgnorePropertyType%"
        },
        "php-docblocker2.unknownType": {
          "type": "string",
          "default": "mixed",
          "enum": [
            "[type]",
            "mixed"
          ],
          "description": "%option.unknownType%"
        },
        "php-docblocker2.defaultParameterDescription": {
          "type": "boolean",
          "default": false,
          "description": "%option.defaultParameterDescription%"
        },
        "php-docblocker2.defaultMessage": {
          "type": "string",
          "default": "name",
          "enum": [
            "blank",
            "name",
            "undocumented"
          ],
          "enumDescriptions": [
            "%option.defaultMessage.blank%",
            "%option.defaultMessage.name%",
            "%option.defaultMessage.undocumented%"
          ],
          "description": "%option.defaultMessage%"
        },
        "php-docblocker2.gap": {
          "type": "boolean",
          "default": true,
          "description": "If there should be a gap between the description and tags"
        },
        "php-docblocker2.returnGap": {
          "type": "boolean",
          "default": false,
          "description": "If there should be a gap between params and return"
        },
        "php-docblocker2.returnVoid": {
          "type": "boolean",
          "default": true,
          "description": "Should we return void if there is no detectable return type?"
        },
        "php-docblocker2.extra": {
          "type": "array",
          "default": [],
          "description": "Extra tags you wish to include in every DocBlock"
        },
        "php-docblocker2.useShortNames": {
          "type": "boolean",
          "default": true,
          "description": "%option.useShortNames%"
        },
        "php-docblocker2.qualifyClassNames": {
          "type": "boolean",
          "default": false,
          "description": "Fully qualifies any data types used in param and returns by reading the namespaces."
        },
        "php-docblocker2.functionTemplate": {
          "type": "object",
          "default": null,
          "description": "Specify the default template for functions."
        },
        "php-docblocker2.propertyTemplate": {
          "type": "object",
          "default": null,
          "description": "Specify the default template for class variables."
        },
        "php-docblocker2.classTemplate": {
          "type": "object",
          "default": null,
          "description": "Specify the default template for classes."
        },
        "php-docblocker2.variableTemplate": {
          "type": "object",
          "default": null,
          "description": "Specify the default template for variables."
        },
        "php-docblocker2.variableInline": {
          "type": "boolean",
          "default": true,
          "description": "%option.variableInline%"
        },
        "php-docblocker2.variableWithKey": {
          "type": "boolean",
          "default": true,
          "description": "%option.variableWithKey%"
        },
        "php-docblocker2.author": {
          "type": "object",
          "default": {
            "name": "Name",
            "email": "email@email.com"
          },
          "description": "Default author tag"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^6.0.40",
    "@types/vscode": "^1.17",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "typescript": "^3.5.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "coveralls": "^3.0.7"
  }
}